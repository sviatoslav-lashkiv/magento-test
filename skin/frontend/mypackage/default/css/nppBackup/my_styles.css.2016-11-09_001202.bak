/* GLOBAL */
.wrapper {
	background:#ececec url("../images/background.jpg") !important;
	font-family: Arial, Helvetica, sans-serif; 
	font-size: 12px;
}
.container {
	width: 1050px;
}

/*=============
HEADER
==============*/
.header {
	padding-top: 20px;
}
.top-header-nav  li  {
	display: inline-block;
	line-height: 2em;
}
.top-header-nav  li a{
	padding: 0.5em;
	color: #535355;
}
.form-language label {
	font-weight: normal;
}
.top-header-lang select {
	border: none;
	background: none;
	color: #6fcfbc;
	outline: none;
	cursor: pointer;
}
.main-header {
	height: 50px;
	background-color:#585858;
}
.main-nav a, .welcome-title p{
	color: #ececec;
}
.main-nav  li>a:hover, .main-nav li>a:visited{
	background-color: #848484;
	color: #ececec;
}
.welcome-title p{
	line-height: 50px;
	padding-left: 20px;
	margin: 0;
}
.bottom-header {
	background-color:#FFF;
	display: flex;
	height: 142px;
}
.company-logo img{
	height: 142px;
	width: 170px;
}
.bottom-nav {
	width: 100%;
}
.search-input {
	background-color:#ececec;
	padding: 6px;
}
.search-input  #search , .search-input button{
	height: 32px;
}
.search-input #search {
	width: 88%;
	padding: 1px 0;
	border: 1px solid #dbdbdb;
}
.search-input button{
	width: 12%;
	color: #ececec;
	background-color:#585858;
	border: none;
}
.bottom-menu {
	height: 90px;
}
.catalog-nav .nav {
	padding-right: 30px;
	padding-top: 20px;
}

/* Catalog Navigation */
/* Dropdown Level 1*/
li.view-all {
	display: none !important;
}
#nav > ol > li.level0 {
	float: right;
	margin-right: 15px;
}

#nav > ol > li.level0 > a.level0 {
  color: #535355;
  font-weight: bold;
	font-size: 16px;
	text-transform: none;
	padding: 0 7px 0 20px;
}
#nav > ol > li.level0 > a.level0:hover {
  color: #949494;
}
ul.level0 a {
	font-size: 14px;
}
.arrow-dwn:before {
	content: "\25BC";
}
/* Dropdown Level-2 */
.nav-primary li.level0 ul.level0 {
	max-width: 200px;
  padding: 0;
  border: 1px solid #bdbdbd;
}
.nav-primary li.level1 > a.level1 {
  padding: 7px 15px;
  background-color: #f3f3f3;
  border-bottom: 1px solid #bdbdbd;
}
.nav-primary li.level1 > a.level1:hover {
  background-color: #6FCFBC;
	color: #535355;
}
/*Dropdown Level-3*/
.nav-primary li.level1 > ul.level1 {
  top: 0;
  left: 80%;
  margin-top: 20px;
	max-width: 150px;
  padding: 0;
  border: 1px solid #bdbdbd;
}
.nav-primary li.level1 > ul.level1>li.level2 {
  background-color: #dcdcdc;
}
.nav-primary li.level1>ul.level1>li.level2>a.level2 {
  padding: 7px 15px;
  border-bottom: 1px solid #bdbdbd;
}
.nav-primary li.level1>ul.level1>li.level2>a.level2:hover {
	background-color: #c3c3c3;
}

/*=============
CONTENT BLOCK
==============*/
.content-block {
	padding-bottom: 30px;
}
.content-block, .footer-block{
	background-color:#FFF;
}
.padding-top-30 {
	padding-top: 30px;
}

/*=============
FOOTER
==============*/
.footer-block {
	color: #909090;
	margin-bottom: 20px;
}
.footer-nav-group {
	padding: 15px 0;
	border-top: 1px solid #e1e1e1;
	border-bottom: 1px solid #e1e1e1;
}
.footer-nav-block {
	padding-left: 20px;
}
.footer-nav-block h4 {
 color: #585858;
 font-size: 14px;
}
.footer-nav-block li {
	padding: 2px 0;
}
.footer-nav-block a {
	color: #bebebe;
}
.footer-nav-block a:hover {
	text-decoration:none;
	color: #70CEBC;
}
.footer-nav-block address {
	margin: 10px 0;
	padding: 0;
	text-align: left;
	border: none;
}
.footer-nav-block .contacts-phone {
	font-size: 14px;
	color: #70CEBC;
	font-weight: bold;
}
.footer-block .copyright {
	text-align: left;
	padding: 15px 0;
	margin: 0;
}
.footer-block  .sm-list img {
	padding-left: 10px;
}



.search-autocomplete:before  {
	display: none !important;
}
#header-nav {
	border: none !important;
}




/* Right column */
.compare-products-title {
	margin-top: 25px;
	font-size: 16px;
	font-weight: bold;
	color: #636363;
}
.compare-products-text {
	color: #636363;
	font-size: 14px;
}